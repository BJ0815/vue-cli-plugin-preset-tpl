{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": [
        "save",
        "commit"
      ]
    },
    "vue-cli-plugin-element": {
      "prompts": true
    },
    "vue-cli-plugin-preset-tpl": {
      "replaceTemplates": true
    }
  },
  "router": true,
  "routerHistoryMode": false,
  "vuex": true,
  "cssPreprocessor": "node-sass"
}